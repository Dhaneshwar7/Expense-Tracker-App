<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>Dashboard</title>
		<link rel="stylesheet" href="../stylesheets/dashboard.css" />
		<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
		<link
			href="https://cdn.jsdelivr.net/npm/remixicon@3.6.0/fonts/remixicon.css"
			rel="stylesheet"
		/>
	</head>
	<body>
		<main id="main">
			<div class="left-container">
				<div class="left-desktop">
					<div class="logo">
						<picture class="logo-pic" style="width: 100%; height: 100%">
							<img
								src="../images/logo.webp"
								alt=""
							/>
						</picture>
						<h1 class="logo-name">Mooney</h1>
					</div>
					<nav class="navigation">
						<ul class="nav-ul">
							<li>
								<a href="/">
									<i class="ri-home-7-fill"></i>
									<span>Dashboard </span>
								</a>
							</li>
							<li>
								<a href="/wallet">
									<i class="ri-wallet-3-fill"></i>
									<span>Wallet</span>
								</a>
							</li>
							<li>
								<a href="/transaction">
									<i class="ri-bar-chart-fill"></i>
									<span>Transaction</span>
								</a>
							</li>
							<li>
								<a href="/profile">
									<i class="ri-user-2-fill"></i>
									<span>Profile</span>
								</a>
							</li>
							<li>
								<a href="/settings">
									<i class="ri-settings-5-fill"></i>
									<span>Settings</span>
								</a>
							</li>
						</ul>
					</nav>
				</div>
			</div>
			<div class="center-container">
				<div class="search-box">
					
				</div>
			</div>
			<div class="right-container">
				<div class="right-desktop">
					<div class="right-top">
						<div class="right-icons">
							<div class="setting-btn">
								<i class="ri-settings-5-fill menuBtn"></i>
							</div>
							<div class="close-menu">
								<div class="open-btn obb">
									<i class="ri-menu-3-line menuBtn"></i>
								</div>
								<div class="close-btn obb">
									<i class="ri-close-fill menuBtn"></i>
								</div>
							</div>
						</div>
					</div>
					<div class="profile-container">
						<div class="profile-pic">
							<div class="dp"></div>
							<div class="profile-name">Dhaneshwar Marskole</div>
						</div>
						<ul class="total-expense">
							<li>
								<i class="ri-calendar-close-line"></i>
								<span>Top Up</span>
							</li>
							<li>
								<i class="ri-calendar-close-line"></i>
								<span>Top Up</span>
							</li>
							<li>
								<i class="ri-calendar-close-line"></i>
								<span>Top Up</span>
							</li>
							<li>
								<i class="ri-add-circle-fill"></i>
								<span>Add</span>
							</li>
						</ul>
						<div class="recent-transaction">
							<div class="recent-title">
								
							</div>
						</div>
					</div>
				</div>
			</div>
		</main>
		<script src="../javascripts/dashboard.js"></script>
	</body>
</html>
